@article{Mavriplis2002,
abstract = {The relative performance of a non-linear FAS multigrid algorithm and all equivalent linear multigrid algorithm for solving two different non-linear problems is investigated. The first case consists of a transient radiation-diffusion problem for which an exact linearization is available, while the second problem involves the solution of the steady-state Navier-Stokes equations, where a first-order discrete Jacobian is employed as an approximation to the Jacobian of a second-order accurate discretization. When an exact linearization is employed, the linear and non-linear multigrid methods converge at identical rates, asymptotically, and the linear method is found to he more efficient due to its lower cost per cycle. When an approximate linearization is employed, as in the Navier-Stokes cases, the relative efficiency of the linear approach versus the non-linear approach depends both on the degree to which the linear system approximates the full Jacobian as well as the relative cost of linear versus non-linear multigrid cycles. For cases where convergence is limited by a poor Jacobian approximation. substantial speedup can be obtained using either multigrid method as a preconditioner to a Newton-Krylov method.},
author = {Mavriplis, Dimitri J.},
doi = {10.1006/jcph.2001.6948},
file = {:home/marchdf/papers/Mavriplis - 2002 - Journal of Computational Physics:},
isbn = {0021-9991},
issn = {00219991},
journal = {J. Comput. Phys.},
keywords = {Krylov,krylov,multigrid,unstructured},
month = {jan},
number = {1},
pages = {302--325},
title = {{An Assessment of Linear Versus Nonlinear Multigrid Methods for Unstructured Mesh Solvers}},
url = {http://hdl.handle.net/2060/20010062298 http://linkinghub.elsevier.com/retrieve/pii/S0021999101969487},
volume = {175},
